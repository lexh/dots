export ZSH=$HOME/.oh-my-zsh
ZSH_THEME="robbyrussell"
plugins=(git python pep8 pylint history-substring-search)
export EDITOR="/usr/bin/vim"
export PATH="/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin"
export PATH="$PATH:$HOME/tools/arcanist/bin"
export PATH="$PATH:/usr/local/mysql/bin"
export PATH="$PATH:/usr/local/go/bin"
export PATH="$PATH:/Users/lex/bin"
export PATH="$PATH:/Users/lex/git/ltools/bin"
export MANPATH="/usr/local/man:$MANPATH"
source $ZSH/oh-my-zsh.sh
export LANG=en_US.UTF-8
export PYENV_ROOT=/usr/local/opt/pyenv
eval "$(pyenv init -)"
export GOPATH="$HOME/go"
setopt HIST_IGNORE_ALL_DUPS
setopt INC_APPEND_HISTORY
setopt SHARE_HISTORY
setopt HIST_VERIFY
export HISTFILE=~/.histfile
export HISTSIZE=90000
export SAVEHIST=90000
export HISTORY_IGNORE="(ls|ll|la|pwd|exit|clear|clr|h *|history)"
h() {
    if [ -z "$*" ]; then
        history;
    else
        res=$(history)
        for arg in "$@"; do
            res=$(echo $res | egrep $arg | grep -v " h ")
        done;
    echo $res
    fi;
}
p() { ps aux | egrep -v egrep | GREP_COLOR='01;34' egrep --color=always -i "$@"; }
pid() { p "$@" | sed -E 's/ +/ /g' | cut -d ' ' -f2 }
fetch() {
    source ~/git/fiesta/venv/bin/activate;
    ~/git/fiesta/fiesta/cloudfront.py $@ | xargs -n1 curl --silent;
    deactivate;
}
fsign() {
    curl -G 'http://fiesta-sign.test.hulu.com/sign' --data-urlencode "url=$@"
}
dgo() {
    URL=$(donki.py "$@" url)
    ret=$?
    echo $URL
    if (( ret != 0 )); then
        return
    fi;
    open $URL
}
bdiff() {
    git log --graph --pretty=format:'%Cred%h%Creset (%ae) -%C(yellow)%d%Creset %s %Cgreen(%cr)%Creset' --abbrev-commit --date=relative $1..$2
}
bindkey -v
export KEYTIMEOUT=1
alias sf='cd /Users/lex/go/src/github.prod.hulu.com/ContentPlatform/stormflow.git'
sfpath=~/go/src/github.prod.hulu.com/stormflow
alias f5='python ~/tmp/fiesta_last_5_min.py | pbcopy'
alias v='source ./venv/bin/activate'
export PATH="$HOME/.jenv/bin:$PATH"
eval "$(jenv init -)"
source /Users/lex/.gvm/scripts/gvm

# The next line updates PATH for the Google Cloud SDK.
if [ -f /Users/lex/Downloads/google-cloud-sdk/path.zsh.inc ]; then
  source '/Users/lex/Downloads/google-cloud-sdk/path.zsh.inc'
fi

# The next line enables shell command completion for gcloud.
if [ -f /Users/lex/Downloads/google-cloud-sdk/completion.zsh.inc ]; then
  source '/Users/lex/Downloads/google-cloud-sdk/completion.zsh.inc'
fi

export PATH="/usr/local/opt/opencv@2/bin:$PATH"
export PKG_CONFIG_PATH="/usr/local/opt/opencv@2/lib/pkgconfig"
